<!DOCTYPE html>
<html lang="hr">
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Mehun.hr{% endblock %}</title>
        {% block metatags %}
            {# enter meta description and keywords:
                <meta name="keywords" content="MEHUN, dental technique laboratory Croatia, dental technician Croatia, laboratory, dentistry, dental, dental clinic, dental bridge, zirconium oxide, teeth, dental implants, metal-ceramic, nonmetal ceramics, inlays, onlays, crowns, dentures, KISS"/>
                <meta name="description" content="Mehun d.o.o. | Dental technique laboratory" />
             #}
        {% endblock %}
        <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
        <meta name="format-detection" content="telephone=no" />
        <meta name="robots" content="index,follow" />
        <!-- Open Graph -->
        <meta property="og:title" content="Mehun d.o.o." />
        <meta property="og:type" content="article" />
        <meta property="og:url" content="http://www.mehun.hr/" />
        <meta property="og:image" content="http://mehun.hr/images/facebook.png"/>
        <meta property="og:description" content=" Dental laboratory Mehun d.o.o. - Denture repairs, CAD / CAM, Wax Up, Smile design and other dental technique services [ Varazdin, Croatia, Ivana Mazuranica 70 ]"  />        <!-- Twitter Card -->
        <meta name="twitter:card" value="summary">
        {% block stylesheets %}
            <link href="{{ asset('http://fonts.googleapis.com/css?family=Istok+Web:400,700&subset=latin,latin-ext') }}" type="text/css" rel="stylesheet" />
            <link href="{{ asset('style/reset.css') }}" type="text/css" rel="stylesheet" />
            <link href="{{ asset('style/superfish.css') }}" type="text/css" rel="stylesheet" />
            <link href="{{ asset('style/fancybox/jquery.fancybox.css') }}" type="text/css" rel="stylesheet" />
            <link href="{{ asset('style/style.css') }}" type="text/css" rel="stylesheet" />
            <link href="{{ asset('style/responsive.css') }}" type="text/css" rel="stylesheet" />
        {% endblock %}
        <link rel="shortcut icon" href="{{ asset('images/favicon.ico') }}" />
    </head>
    <body>
        <div class="preheader">
            <div class="preheader_info">
                <h5>I. Mazuranica 70, Varazdin, Phone: +385 42 240 366, E-mail: info@mehun.hr </h5>

                <div class="hr_jezik">
                    <a class="ikona_jezika cro" href="{{ path('mehun_content_homepage') }}" title=""> </a>
                </div>
            </div>
        </div>

        <div class="site_container narrow">
            <div class="header_container">
                <div class="header clearfix">
                    <div class="header_left"> <a href="{{ path('mehun_eng_content_homepage') }}" title="Mehun d.o.o."> <img src="{{ asset('images/mehun.png') }}" alt="Mehun logo"/> </a> </div>

                    <!-- Navigacija
                    ================================================== -->
                    <ul class="sf-menu header_right">
                        <li class="submenu selected"> <a href="{{ path('mehun_eng_content_homepage') }}">HOME</a>
                            <ul>
                                <li> <a href="{{ path('about') }}">About us</a> </li>
                            </ul>
                        <li> <a href="#"> SERVICES </a>
                            <ul>
                                <li> <a href="{{ path('repairs') }}">Denture repairs</a> </li>
                                <li> <a href="{{ path('cad_cam_eng') }}"> CAD / CAM </a> </li>
                                <li> <a href="{{ path('wax_up_eng') }}"> Wax Up </a>
                                <li> <a href="{{ path('smile_design_eng') }}"> Smile design </a> </li>
                            </ul>
                        <li> <a href="#"> PRODUCTS </a>
                            <ul>
                                <li> <a href="{{ asset('pdf/mehun_katalog.pdf') }}" target="_blank">Product catalogue</a> </li>
                                <li> <a href="{{ path('gallery') }}"> Gallery </a> </li>
                            </ul>
                        <li> <a href="{{ path('partners') }}"> PARTNERS </a> </li>
                        <li> <a href="#">DOCUMENTS</a>
                            <ul>
                                <li> <a href="{{ path('pricelist') }}">Pricelist</a> </li>
                                <li> <a href="{{ path('certificates') }}">Certificates</a> </li>
                                <li> <a href="{{ asset('pdf/ugovor_o_suradnji.pdf') }}" target="_blank">Cooperation contract</a> </li>
                                <li> <a href="{{ path('quality_policy') }}">Quality policy</a> </li>
                                <li> <a href="{{ path('technical_documentation') }}">Technical documentation</a> </li>
                                <li> <a href="{{ path('questionnaire') }}">Questionnaire</a> </li>
                            </ul>
                        <li> <a href="{{ path('contact') }}">CONTACT</a> </li>

                    </ul>
                    <ul class="sf-menu header_right" style="margin-top:-50px;">
                        {% if is_granted('ROLE_ADMIN') %}
                            <li><a href="{{ path('cms_page') }}"> CMS </a></li>
                        {% elseif is_granted('ROLE_USER')%}
                            <li><a href="{{ path('cms_page') }}"> CMS </a></li>
                        {% endif %}
                        {% if is_granted('ROLE_USER') or is_granted('ROLE_ADMIN') %}
                            <li> <a href="{{ path('logout') }}"> ODJAVA </a> </li>
                        {% endif %}
                    </ul>

                    <!-- Mobilna navigacija
                    ================================================== -->
                    <div class="mobile_menu">
                        <select>
                            <option value="-">-</option>
                            <option value="{{ path('mehun_eng_content_homepage') }}" selected="selected">HOME</option>
                            <option value="{{ path('about') }}">- About us</option>
                            <option value="#">SERVICES</option>
                            <option value="{{ path('repairs') }}">- Repairs</option>
                            <option value="{{ path('cad_cam_eng') }}">- CAD / CAM</option>
                            <option value="{{ path('wax_up_eng') }}">- Wax Up</option>
                            <option value="{{ path('smile_design_eng') }}">- Smile design</option>
