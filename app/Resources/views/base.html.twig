<!DOCTYPE html>
<html lang="hr">
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Mehun.hr{% endblock %}</title>
        {% block metatags %}
            {# enter meta description and keywords:
                <meta name="keywords" content="MEHUN, stomatologija, zubotehnički, laboratorij, dental, dentalne ordinacije, Varaždin, zubni tehničar, zubotehnički laboratorij, zubni, most, cirkon oksid, zubi, implantati, metal-keramika, bezmetalna keramika, inlay, krunica, eclipse, proteza, KISS"/>
                <meta name="description" content="Mehun d.o.o. | poduzeće za zdravstvenu djelatnost zubotehničkog laboratorija" />
             #}
        {% endblock %}
        <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
        <meta name="format-detection" content="telephone=no" />
        <meta name="robots" content="index,follow" />
        <!-- Open Graph -->
        <meta property="og:title" content="Mehun d.o.o." />
        <meta property="og:type" content="article" />
        <meta property="og:url" content="http://www.mehun.hr/" />
        <meta property="og:image" content="http://mehun.hr/images/facebook.png"/>
        <meta property="og:description" content=" Dentalni laboratorij Mehun d.o.o. - Usluge reparatura, CAD / CAM, Wax Up, Smile design i ostale usluge s područja dentalne tehnike [ Ivana Mažuranića 70, Varaždin ]"  />
        <!-- Twitter Card -->
        <meta name="twitter:card" value="summary">
        {% block stylesheets %}
            <link href="{{ asset('http://fonts.googleapis.com/css?family=Istok+Web:400,700&subset=latin,latin-ext') }}" type="text/css" rel="stylesheet" />
            <link href="{{ asset('style/reset.css') }}" type="text/css" rel="stylesheet" />
            <link href="{{ asset('style/superfish.css') }}" type="text/css" rel="stylesheet" />
            <link href="{{ asset('style/fancybox/jquery.fancybox.css') }}" type="text/css" rel="stylesheet" />
            <link href="{{ asset('style/style.css') }}" type="text/css" rel="stylesheet" />
            <link href="{{ asset('style/responsive.css') }}" type="text/css" rel="stylesheet" />
        {% endblock %}
        <link rel="shortcut icon" href="{{ asset('images/favicon.ico') }}" />
    </head>
    <body>
        <div class="preheader">
            <!-- LINK ZA PODRUŽNICU DOLAZI USKORO
            <div class="preheader_site">
                <h5>Podružnica</h5>
            </div>-->
            <div class="preheader_info">
                <h5>I. Mažuranića 70, Varaždin, Tel: +385 42 240 366, E-mail: info@mehun.hr </h5>

                <div class="eng_jezik">
                    <a class="ikona_jezika eng" href="{{ path('mehun_eng_content_homepage') }}" title=""> </a>
                </div>
            </div>
        </div>

        <div class="site_container narrow">
            <div class="header_container">
                <div class="header clearfix">
                    <div class="header_left"> <a href="{{ path('mehun_content_homepage') }}" title="Mehun d.o.o."> <img src="{{ asset('images/mehun.png') }}" alt="Mehun logo"/> </a> </div>

                    <!-- Navigacija
                    ================================================== -->
                    <ul class="sf-menu header_right">
                        <li class="submenu selected"> <a href="{{ path('mehun_content_homepage') }}"> NASLOVNA </a>
                            <ul>
                                <li> <a href="{{ path('o_nama') }}"> O nama </a> </li>
                            </ul>
                        <li> <a href="#"> USLUGE </a>
                            <ul>
                                <li> <a href="{{ path('reparature') }}"> Reparature </a> </li>
                                <li> <a href="{{ path('cad_cam') }}"> CAD / CAM </a> </li>
                                <li> <a href="{{ path('wax_up') }}"> Wax Up </a>
                                <li> <a href="{{ path('smile_design') }}"> Smile design </a> </li>
                            </ul>
                        <li> <a href="#"> PROIZVODI </a>
                            <ul>
                                <li> <a href="{{ asset('pdf/mehun_katalog.pdf') }}" target="_blank"> Katalog proizvoda </a> </li>
                                <li> <a href="{{ path('galerija') }}"> Galerija </a> </li>
                            </ul>
                        <li> <a href="{{ path('partneri') }}"> PARTNERI </a> </li>
                        <li> <a href="#"> DOKUMENTI </a>
                            <ul>
                                <li> <a href="{{ path('cjenik') }}"> Cjenik </a> </li>
                                <li> <a href="{{ path('certifikati') }}"> Certifikati </a> </li>
                                <li> <a href="{{ asset('pdf/ugovor_o_suradnji.pdf') }}" target="_blank"> Ugovor o suradnji </a> </li>
                                <li> <a href="{{ path('politika_kvalitete') }}"> Politika kvalitete </a> </li>
                                <li> <a href="{{ path('tehnicka_dokumentacija') }}"> Tehnička dokumentacija </a> </li>
                                <li> <a href="{{ path('anketni_upitnik') }}"> Anketni upitnik </a> </li>
                            </ul>
                        <li> <a href="{{ path('kontakt') }}"> KONTAKT </a> </li>

                    </ul>
                    <ul class="sf-menu header_right" style="margin-top:-50px;">
                        {% if is_granted('ROLE_ADMIN') %}
                            <li><a href="{{ path('cms_page') }}"> CMS </a></li>
                        {% elseif is_granted('ROLE_USER')%}
                            <li><a href="{{ path('cms_page') }}"> CMS </a></li>
                        {% endif %}
                        {% if is_granted('ROLE_USER') or is_granted('ROLE_ADMIN') %}
                            <li> <a href="{{ path('logout') }}"> ODJAVA </a> </li>
                        {% endif %}
                    </ul>

                    <!-- Mobilna navigacija
                    ================================================== -->
                    <div class="mobile_menu">
                        <select>
                            <option value="-">-</option>
                            <option value="{{ path('mehun_content_homepage') }}" selected="selected">NASLOVNA</option>
                            <option value="{{ path('o_nama') }}">- O nama</option>
                            <option value="#">USLUGE</option>
                            <option value="{{ path('reparature') }}">- Reparature</option>
                            <option value="{{ path('cad_cam') }}">- CAD / CAM</option>
                            <option value="{{ path('wax_up') }}">- Wax Up</option>
                            <option value="{{ path('smile_design') }}">- Smile design</option>
                            <option value="#">PROIZVODI</option>
                            <option value="{{ asset('pdf/mehun_katalog.pdf') }}">- Katalog proizvoda</option>
                            <option value="{{ path('galerija') }}">- Galerija</option>
                            <option value="{{ path('partneri') }}">PARTNERI</option>
                            <option value="#">DOKUMENTI</option>
                            <option value="{{ path('cjenik') }}">- Cjenik</option>
                            <option value="{{ path('certifikati') }}">- Certifikati</option>
                            <option value="{{ asset('pdf/ugovor_o_suradnji.pdf') }}">- Ugovor o suradnji</option>
                            <option value="{{ path('politika_kvalitete') }}">- Politika kvalitete</option>
                            <option value="{{ path('anketni_upitnik') }}">- Anketni upitnik</option>
                            <option value="{{ path('tehnicka_dokumentacija') }}">- Tehnička dokumentacija</option>
                            <option value="{{ path('kontakt') }}">KONTAKT</option>
                        </select>
                    </div>
                </div>
            </div>
        {% block body %}{% endblock %}
        {% block javascripts %}
            <script src="{{ asset('js/jquery-1.8.3.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('js/jquery.ba-bbq.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('js/jquery-ui-1.9.2.custom.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('js/jquery.easing.1.3.js') }}" type="text/javascript"></script>
            <script src="{{ asset('js/jquery.carouFredSel-5.6.4-packed.js') }}" type="text/javascript"></script>
            <script src="{{ asset('js/jquery.sliderControl.js') }}" type="text/javascript"></script>
            <script src="{{ asset('js/jquery.timeago.js') }}" type="text/javascript"></script>
            <script src="{{ asset('js/jquery.hint.js') }}" type="text/javascript"></script>
            <script src="{{ asset('js/jquery.isotope.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('js/jquery.isotope.masonry.js') }}" type="text/javascript"></script>
            <script src="{{ asset('js/jquery.fancybox-1.3.4.pack.js') }}" type="text/javascript"></script>
            <script src="{{ asset('http://maps.google.com/maps/api/js?sensor=false') }}" type="text/javascript"></script>
            <script src="{{ asset('js/main.js') }}" type="text/javascript"></script>
        {% endblock %}
